{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "singularName": "exercise",
    "pluralName": "exercises",
    "displayName": "exercise",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "details": {
      "type": "json"
    },
    "title": {
      "type": "string"
    },
    "time": {
      "type": "float"
    },
    "image_url": {
      "type": "string"
    },
    "muscle": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::muscle.muscle"
    },
    "difficulty": {
      "type": "decimal",
      "max": 5,
      "min": 0
    },
    "accessories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::accessory.accessory",
      "mappedBy": "exercises"
    },
    "description": {
      "type": "text"
    },
    "dashboard_item": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "cat_sch": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cat-sch.cat-sch",
      "inversedBy": "exercises"
    },
    "banner_image_urls": {
      "type": "json",
      "required": false
    }
  }
}
